/*
Create a table named DSN8C10.DEPT in the table space DSN8S12D of the database DSN8D12A.
*/

   CREATE TABLE DSN8C10.DEPT
     (DEPTNO   CHAR(3)     NOT NULL,
      DEPTNAME VARCHAR(36) NOT NULL,
      MGRNO    CHAR(6)             ,
      ADMRDEPT CHAR(3)     NOT NULL,
      LOCATION CHAR(16)            ,
      PRIMARY KEY(DEPTNO)          )
     IN DSN8D12A.DSN8S12D;
